<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
   
    xmlns:tools="http://schemaandroid.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.topbar.MainActivity"
     xmlns:app="http://schemas.android.com/apk/res/com.example.topbar">

    <com.example.topbar.Topbar
        android:id="@+id/topbar1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        
        app:title="金克拉"
        app:titleTextSize="15dp"
        app:titleTextColor="#FFFFFF"
      
        app:leftText="Back"
        app:leftTextColor="#FFFFFF"
        app:leftBackground="#123312"
        
        app:rightText="More"
        app:rightTextColor="#FFFFFF"
        app:rightBackground="#123312"
        
         >
    </com.example.topbar.Topbar>

</RelativeLayout>
